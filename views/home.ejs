     function drawChart() {
       var data = google.visualization.arrayToDataTable([
          ["Time", "Active Power", "Reactive Power", "Apparent Power"],
          [new Date(), <%=ems_t1[0].W_sum%>, <%=ems_t1[0].VA_sum%>, <%=ems_t1[0].VAR_sum%>]
       ]);

       var options = {
         title: 'Active Power Vs. Reactive Power Vs. Apparent Power',
         hAxis: {title: 'Year',  titleTextStyle: {color: '#333'}},
         vAxis: {minValue: 200}
       };

       var chart = new google.visualization.AreaChart(document.getElementById('chartContainer'));
       <%ems_t1.forEach((item) => {%>
         setInterval(()=>{
           data.addRow([new Date(), <%=item.W_sum%>, <%=item.VA_sum%>, <%=item.VAR_sum%>]);
           chart.draw(data, options);
         },1000);
      <%});%>
     }
   </script>
