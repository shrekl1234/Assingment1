                  </div>
                </div>


                <script type="text/javascript">
                  $('.knob').each(function() {

                    var $this = $(this);
                    var myVal = $this.attr("rel");
                    $this.knob();

                    $({
                      value: 0
                    }).animate({
                      value: myVal
                    }, {
                      duration: 2000,
                      easing: 'swing',
                      step: function() {
                        $this.val(Math.ceil(this.value)).trigger('change');

                      }
                    })

                  });
                </script>
